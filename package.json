{
  "name": "@dosgato/dialog-ckeditor",
  "description": "A form control that provides an instance of CKEditor. Designed to be compatible with @dosgato/dialog.",
  "version": "0.0.12",
  "scripts": {
    "prepublishOnly": "echo \"Do not publish from the root. Run npm run package and publish the package directory.\"; exit 1;",
    "dev": "vite dev",
    "devck": "cp -R ~/Software/dosgato-ckeditor/build node_modules/@txstate-mws/dosgato-ckeditor && rm -rf node_modules/.vite && vite dev",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "@dosgato/ckeditor": "^1.0.0",
    "@dosgato/dialog": ">=0.0.35",
    "@iconify-icons/mdi": "^1.2.38",
    "@iconify/svelte": "^3.0.1",
    "@txstate-mws/svelte-forms": "^1.3.4",
    "sass": "^1.58.0",
    "svelte": "^3.46.6",
    "txstate-utils": "^1.8.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/package": "^1.0.0",
    "@types/ckeditor__ckeditor5-build-classic": "^29.0.1",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "author": "Nick Wing",
  "license": "GPL-2.0-or-later",
  "bugs": "https://github.com/txstate-etc/dosgato-dialog-ckeditor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/txstate-etc/dosgato-dialog-ckeditor.git"
  }
}
