{
  "name": "@dosgato/dialog-ckeditor",
  "description": "A form control that provides an instance of CKEditor. Designed to be compatible with @dosgato/dialog.",
  "version": "1.0.2",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepublishOnly": "svelte-package",
    "dev": "vite dev",
    "devck": "cp -R ~/Software/dosgato-ckeditor/build node_modules/@txstate-mws/dosgato-ckeditor && rm -rf node_modules/.vite && vite dev",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "@dosgato/ckeditor": "^1.1.7",
    "@dosgato/dialog": ">=0.0.79",
    "@iconify-icons/mdi": "^1.2.38",
    "@txstate-mws/svelte-forms": "^1.3.13",
    "txstate-utils": "^1.8.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/package": "^2.0.0",
    "@ckeditor/ckeditor5-build-classic": "^35.4.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "sass": "^1.58.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.6.0",
    "tslib": "^2.3.1",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "svelte": "^3.55.1 || ^4.0.0"
  },
  "files": ["dist"],
  "type": "module",
  "author": "Nick Wing",
  "license": "GPL-2.0-or-later",
  "bugs": "https://github.com/txstate-etc/dosgato-dialog-ckeditor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/txstate-etc/dosgato-dialog-ckeditor.git"
  }
}
